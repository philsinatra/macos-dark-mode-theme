  "tokenColors": [
    {
      "settings": {
        "background": "#000000",
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#7f8c98",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#ff8170"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#f977ae"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#f977ae"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#f977ae"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#f977ae"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "",
        "foreground": "#7ae3f7"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "foreground": "#f977ae"
      }
    },
    {
      "scope": [
        "entity.name",
        "entity.static",
        "entity.name.class.static.function",
        "entity.name.function",
        "entity.name.class",
        "entity.name.type",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#7ae3f7"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#e1efff"
      }
    },
    {
      "name": "Variable names that are specified by the language",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "",
        "foreground": "#7ae3f7"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f977ae"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#7ae3f7"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "fontStyle": "",
        "foreground": "#7ae3f7"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.class"],
      "settings": {
        "foreground": "#7ae3f7"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": "",
        "foreground": "#000000"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "background": "#F9267200",
        "fontStyle": "",
        "foreground": "#adcf38"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "background": "#AE81FF00",
        "foreground": "#adcf38"
      }
    },
    {
      "name": "CSS property",
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#99c6ca",
        "fontStyle": "italic"
      }
    },
    {
      "name": "[HTML] - Basic",
      "scope": "text.html.basic",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "[HTML] - Entity Name",
      "scope": "text.html.basic entity.name",
      "settings": {
        "foreground": "#9effff"
      }
    },
    {
      "name": "[HTML] - ID value",
      "scope": "meta.toc-list.id.html",
      "settings": {
        "foreground": "#A5FF90"
      }
    },
    {
      "name": "[HTML] - Entity Other",
      "scope": "text.html.basic entity.other",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffc600"
      }
    },
    {
      "name": "[HTML] - Script Tag",
      "scope": "meta.tag.metadata.script.html entity.name.tag.html",
      "settings": {
        "foreground": "#ffc600"
      }
    },
    {
      "name": "[HTML] - Quotes. these are a slightly different colour because expand selection will then not include quotes",
      "scope": "punctuation.definition.string.begin, punctuation.definition.string.end",
      "settings": {
        "foreground": "#92fc79"
      }
    },
    {
      "name": "Support Variable DOM JS",
      "scope": "support.variable.dom.js",
      "settings": {
        "foreground": "#e1efff"
      }
    },
    {
      "name": "Keyword Operator Assignment JS",
      "scope": "keyword.operator.assignment.js",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Punctuation Definition Block JS",
      "scope": "punctuation.definition.block.js",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Variable Parameter JS",
      "scope": "variable.parameter.js",
      "settings": {
        "foreground": "#e1efff"
      }
    },
    {
      "name": "Entity Name Type JS",
      "scope": "entity.name.type.js",
      "settings": {
        "foreground": "#e1efff"
      }
    },
    {
      "name": "Punctuation Accessor JS",
      "scope": "punctuation.accessor.js",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Support Variable Property DOM JS",
      "scope": "support.variable.property.dom.js",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Punctuation Terminator Statement JS",
      "scope": "punctuation.terminator.statement.js",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
