      }
    },
    {
      "name": "JSON Property Name",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#f977ae"
      }
    },
    {
      "name": "JSON Punctuation Begin",
      "scope": ["punctuation.definition.dictionary.begin.json"],
